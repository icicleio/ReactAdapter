{
    "name": "icicleio/react-adaptor",
    "description": "Adapts the event loop and promises of Icicle to interfaces compatible with components built for React.",
    "keywords": [
        "adaptor",
        "loop",
        "promise"
    ],
    "homepage": "http://icicle.io",
    "license": "Apache-2.0",
    "authors": [
        {
            "name": "Aaron Piotrowski",
            "email": "aaron@icicle.io"
        }
    ],
    "require": {
        "icicleio/icicle": "0.2.*",
        "react/event-loop": "0.4.*",
        "react/promise": "~2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "4.0.*",
        "satooshi/php-coveralls": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "Icicle\\ReactAdaptor\\": "src"
        }
    }
}
